        value="0.063515" />
      <inertia
        ixx="3.2227E-05"
        ixy="-2.691E-07"
        ixz="4.7122E-08"
        iyy="2.4227E-05"
        iyz="-1.2861E-06"
        izz="3.3868E-05" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://thymio_description/meshes/main_body.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.75294 0.75294 0.75294 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
