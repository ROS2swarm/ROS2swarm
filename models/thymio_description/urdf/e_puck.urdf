<link name="base_link"/>

  <joint name= "base_link_to_main_body" type = "fixed">
   <parent link = "base_link"/>
   <child link = "main_body"/>
   <origin rpy="0 0 1.57079633" xyz="0.0 -0.0 0.0"/>
  </joint>


  <link
